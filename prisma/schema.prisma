generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}




model User {
  Id      String   @id @default(auto()) @map("_id") @db.ObjectId
  Token   String   @unique
  Name    String
  Mobile  String   @unique
  Email   String   @unique
  College String
  Section String
  Day1    String
  Day2    String
  Amount  Int
  Country String
  State   String
  Image   String   @unique
  Date    String 
  Status  Boolean  @default(false)
}